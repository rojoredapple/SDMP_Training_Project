<h1> List of Attendees </h1>

<table>
  <thead>
    <tr>
      <th> Emails </th>
      <th> Check in Time </th>
      <th colspan = "3"></th>
    </tr>
  </thead>

  <tbody>
  <% @attendees.each do |attendee| %>
    <tr>
      <td><%= attendee.name %></td>
      <td><%= attendee.created_at %></td>
      <td><%= link_to "Show", attendee %></td>
      <td><%= link_to "Edit", edit_attendee_path(attendee) %></td>
      <td><%= link_to "Cancel", attendee, method: :delete, data: { confirm: "Are you sure?" } %></td>
    </tr>
  <% end %>
  </tbody>
</table>
